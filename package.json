{
  "name": "time-tracking-system",
  "private": true,
  "type": "module",
  "scripts": {
    "firebase:emulators": "firebase emulators:start",
    "emulators:seed": "firebase emulators:start --import=./seed",
    "emulators:export": "firebase emulators:export ./seed",
    "deploy:rules": "firebase deploy --only firestore:rules",
    "deploy:indexes": "firebase deploy --only firestore:indexes",
    "deploy:all": "firebase deploy",
    "deploy:admin": "firebase deploy --only hosting:admin",
    "deploy:hours": "firebase deploy --only hosting:hours",
    "app:deploy": "node scripts/deploy.js",
    "app:deploy:test": "node scripts/deploy-test.js",
    "build:all": "pnpm run build:common && pnpm run build:admin && pnpm run build:hours",
    "build:common": "cd packages/common && pnpm run build",
    "build:admin": "cd packages/admin && pnpm run build",
    "build:hours": "cd packages/hours && pnpm run build",
    "build:ci": "pnpm run edit:fix-typescript && pnpm run build:all",
    "create:empty-builds": "node scripts/create-empty-builds.js",
    "test:deployment": "node scripts/test-deployment.js",
    "fix:dependencies": "node scripts/fix-dependencies.js",
    "fix:jest-dom": "node scripts/fix-jest-dom.js",
    "fix:all": "node scripts/run-all-fixes.js",
    "fix:query-types": "node scripts/fix-query-types.js",
    "fix:build": "node scripts/fix-build.js",
    "fix:test-deps": "node scripts/fix-test-deps.js",
    "fix:module-syntax": "node scripts/fix-module-syntax.js",
    "modernize": "node scripts/modernize-project.js",
    "modernize:components": "node scripts/modernize-components.js",
    "cleanup": "node scripts/cleanup.js",
    "docs:find-duplicates": "node scripts/find-duplicates.js",
    "docs:generate-index": "node scripts/generate-doc-index.js",
    "workflow:check": "node scripts/workflow-check.js",
    "project:audit": "pnpm run docs:generate-index && pnpm run docs:find-duplicates",
    "project:cleanup": "pnpm run cleanup",
    "lint": "eslint packages/*/src/**/*.{ts,tsx} --max-warnings=100",
    "lint:fix": "eslint packages/*/src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"packages/*/src/**/*.{ts,tsx,css,scss,json}\"",
    "test": "pnpm run fix:test-deps && vitest run",
    "test:watch": "pnpm run fix:test-deps && vitest",
    "test:coverage": "pnpm run fix:test-deps && vitest run --coverage",
    "precommit": "pnpm run project:audit && pnpm run lint && pnpm run test",
    "edit:fix-typescript": "node scripts/fix-typescript.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@tailwindcss/postcss": "^4.0.14",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^1.6.1",
    "@vitest/ui": "^1.6.1",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "js-yaml": "^4.1.0",
    "jsdom": "^23.2.0",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3",
    "typescript-eslint": "^7.3.1",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "immer": "^10.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}